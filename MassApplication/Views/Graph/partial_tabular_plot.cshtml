@using MassApplication.Models.Graph;
@model ChartParameters
@using System.Data;


<div>
    <table id="kpitabularData" class="kpiData table table-bordered table-striped" cellspacing="0" rules="all" border="1" style="border-collapse: collapse;">
        <thead>
            <tr>
                @foreach (System.Data.DataColumn col in Model.data_t.Columns)
                { 
                    <th style="background: #343a40; color: #fff; border-radius: 4px;">@col.Caption</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (System.Data.DataRow row in Model.data_t.Rows)
             {
                <tr>
                    @foreach (var cell in row.ItemArray)
                    {
                        <td>@cell.ToString()</td>

                    }
                </tr>
             }
        </tbody>
    </table>
</div>

