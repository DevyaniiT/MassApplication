                        <div class="col-md-6">
                            <div class="row">
                                <label ID="Lbl_frequencyVolume" class="col-sm-4 col-form-label">Frequency: </label>
                                <div class="col-sm-8">
                                    <input list="fcVolume_frequency" name="fcFrequencyVolume" type="text" class="fcTag form-control" placeholder="Search to select" autocomplete="off">
                                    <datalist id="fcVolume_frequency"></datalist>
                                </div>
                            </div>
