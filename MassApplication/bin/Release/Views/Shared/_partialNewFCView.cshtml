@model MassApplication.Models.FCAssetClass

<style>
    .USMTag, #FCChordDiv {
        display: none;
    }

    span.error {
        display: block;
        visibility: hidden;
        color: red;
        font-size: 90%;
    }

    #tabFCTag .nav-link {
        color: #000;
        background-color: #e1e1e1;
        border: 0;
        margin: 0 4px;
        padding: 13px 23px;
        font-size: 13px;
        border-radius: 5px;
        text-transform: uppercase;
        font-weight: 600;
    }

        #tabFCTag .nav-link.active, .nav-tabs .nav-item.show .nav-link {
            color: #ffffff;
            background-color: #2a5b6c;
            border: 0;
            padding: 13px 23px;
            margin: 0 4px;
            border-radius: 5px;
            text-transform: uppercase;
            font-size: 13px;
            font-weight: 600;
        }
</style>

<div class="form-horizontal">
    <div class="master">
        <div class="form-group row">
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_fcname" class="col-sm-4 col-form-label">FC Name: </label>
                    <div class="col-sm-8">
                        <input id="fc_name" type="text" class="form-control" placeholder="Enter FC Name ">
                        <span class="error">FC name required</span>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_network" class="col-sm-4 col-form-label">Network: </label>
                    <div class="col-sm-8">
                        @Html.DropDownList("DDNetwork", Enumerable.Empty<SelectListItem>(), "--SELECT--", new { @class = "form-control" })
                        <span class="error">Network required</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_FCstation" class="col-sm-4 col-form-label">FC Station: </label>
                    <div class="col-sm-8">
                        <div id="fc_stationTag">
                            <input list="fc_station" name="fcStationName" class="form-control" placeholder="Search to select any station name" />
                            <datalist id="fc_station"></datalist>
                            <span class="error">FC station required</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_FCtype" class="col-sm-4 col-form-label">FC Type: </label>
                    <div class="col-sm-8">
                        @Html.DropDownList("DDFCType", new List<SelectListItem>
                                          {
                                              new SelectListItem {Text = "FC", Value = "fc"},
                                              new SelectListItem {Text = "FC_UM", Value = "fc_um" }
                                     }, "--SELECT--", new { @class = "form-control" })
                        <span class="error">FC type required</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_gcname" class="col-sm-4 col-form-label">GC Name: </label>
                    <div class="col-sm-8">
                        @Html.DropDownList("DDGCName", Enumerable.Empty<SelectListItem>(), "--SELECT--", new { @class = "form-control" })
                        <span class="error">GC name required</span>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_metertype" class="col-sm-4 col-form-label">Meter Type: </label>
                    <div class="col-sm-8">
                        @Html.DropDownList("DDMeterType", Enumerable.Empty<SelectListItem>(), "--SELECT--", new { @class = "form-control" })
                        <span class="error">Meter Type required</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group row" id="FCChordDiv">
            <div class="col-md-6">
                <div class="row">
                    <label ID="Lbl_fcchord" class="col-sm-4 col-form-label">Number of Chord: </label>
                    <div class="col-sm-8">
                        <input id="fc_chord" type="text" class="form-control" placeholder="Enter Chord" onfocusout="chordFunction(this)">
                        <span class="error">Chord required</span>
                    </div>
                </div>
            </div>
        </div>

        <ul class="newnavconn nav nav-tabs border-bottom-0" id="tabFCTag">
            <li class="nav-item"><a href="#newVolumeTab" class="nav-link active" data-toggle="tab">Volume Tag</a></li>
            <li class="nav-item"><a href="#newEnergyTab" class="nav-link" data-toggle="tab">Energy Tag</a></li>
            <li class="nav-item USMTag"><a href="#newVOSTab" class="nav-link" data-toggle="tab">VOS</a></li>
            <li class="nav-item USMTag"><a href="#newVOGTab" class="nav-link" data-toggle="tab">VOG</a></li>
            <li class="nav-item USMTag"><a href="#newPerformanceTab" class="nav-link" data-toggle="tab">Performance</a></li>
            <li class="nav-item USMTag"><a href="#newAGCTab" class="nav-link" data-toggle="tab">AGC</a></li>
            <li class="nav-item USMTag"><a href="#newSNRTab" class="nav-link" data-toggle="tab">SNR</a></li>
