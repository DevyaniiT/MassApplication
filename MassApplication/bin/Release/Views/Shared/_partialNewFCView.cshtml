<!-- SCRIPT FOR DROPDOWN EVENT -->
<script>
    $(document).on('change', 'select#DDMeterType', function () {
        var meter_type = $('select[name=DDMeterType]').val();
        if (meter_type == "4") {
            $(".USMTag").css("display", "block");
            $("#FCChordDiv").css("display", "block");
        }
        else {
            $(".USMTag").css("display", "none");
            $("#FCChordDiv").css("display", "none");
            $("#fc_chord").val("");
            $(".ChordWiseDiv").empty();
            $('#tabFCTag li a.active').removeClass('active');
            $('#tabFCTag li a[href="#newVolumeTab"]').addClass('active');
            $(".master .tab-content .active").removeClass("active");
            $(".tab-content #newVolumeTab").addClass('active');
        }
    });
    $(document).on('change', 'select#DDNetwork', function () {
        $("#fc_station").empty();
        //$("#fc_station option").eq(0).remove();
        var networkId = $('select[name=DDNetwork]').val();
        if (networkId != null && networkId != '') {
            $.ajax({
                url: '@Url.Content("~/Asset/GetStationDropdown?id=")' + networkId,
                type: "get",
                dataType: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $("#fc_station").empty();
                    $.each(data, function (i, data) {
                        $('<option>',
                            {
                                value: data.station_short_name,
                            }).html(data.station_short_name).attr("data-val", data.station_id).appendTo("#fc_station");
                    });
                },
                error: function () {
                    alert("Error");
                }
            });
        }
    });
</script>
<!-- SCRIPT FOR DATALIST TAG -->
<script>
    $(document).on('keyup', '.fcTag', function () {
        var currentDiv = $(this).attr('list');
        $("#" + currentDiv).empty();
        var data = (this).value;
        getSearchData(data, currentDiv);
    });
    function getSearchData(key, divName) {
          $.ajax({
            url: '@Url.Content("~/Asset/SearchFCKey?searchKey=")' + key,
            type: "get",
            dataType: "json",
            contentType: 'application/json; charset=utf-8',
              success: function (data) {
                $("#" + divName).empty();
                $.each(data, function (i, data) {
                    $('<option>',
                        {}).html(data.longid).attr("data-val", data.pointid).appendTo("#" + divName);
                });
            },
            error: function () {
                alert("Error");
            }
        });
    }
    function chordFunction(mytext) {
        //console.log('mytext:', mytext);
        var txtdata = mytext.value;
        txtdata = parseInt(txtdata);
        $(".ChordWiseDiv").empty();
        var newDivData = getPathDiv(txtdata);
        //console.log('newDivData:', newDivData);
        $(".ChordWiseDiv").append(newDivData);
    }
    function getPathDiv(txtdata) {
        var newDiv = '';
        var input_name = '';
        for (var i = 0; i < txtdata; i++) {
            input_name = 'fc_path' + i;
            path_name = 'Path ' + (i + 1) + ':';
            newDiv += '<div class="col-md-6 col-sm-6">'
                + '<div class="row">'
                + '<label class="col-sm-4 col-form-label" >' + path_name + '</label>'
                + '<div class="col-sm-8">'
                + '<input list=' + input_name + ' type="text" class="fcTag form-control" placeholder="Search to select" autocomplete="off">'
                + '<datalist id=' + input_name + '></datalist>'
                + '</div></div></div>'
        }
        return newDiv;
    }
</script>


